        function change(data) {
            var name = data.name
            $("#type").val(name);

            $(data).attr("class", "btn btn-primary").siblings().attr("class", "btn btn-default");
            if (name === 'mail') {
                $('#mail').css("display", "block");
                $('#mobile').css("display", "none");
                return;
            }
            $('#mail').css("display", "none");
            $('#mobile').css("display", "block");
        }

