        var countdown = 120;

        function settime() {

            if (countdown === 0) {
                $("#sendCode").attr("disabled", false);
                $("#sendCode").html("获取验证码");
                countdown = 60;
                return;
            } else {
                $("#sendCode").attr("disabled", true);
                $("#sendCode").html(countdown+ "秒后可发");
                countdown--;
            }
            setTimeout(function () {
                    settime(this)
                }
                , 1000)

        }


